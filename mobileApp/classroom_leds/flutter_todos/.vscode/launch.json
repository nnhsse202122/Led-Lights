    "flutterMode": "debug",
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Chrome",
            "request": "launch",
            "type": "pwa-chrome",
            "url": "http://localhost:8080",
            "webRoot": "${workspaceFolder}"
        },
        {
            "name": "flutter_todos",
            "request": "launch",
            "type": "dart"
        },
        {
            "name": "flutter_todos (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "local_storage_todos_api",
            "cwd": "packages/local_storage_todos_api",
            "request": "launch",
            "type": "dart"
        },
        {
            "name": "local_storage_todos_api (profile mode)",
            "cwd": "packages/local_storage_todos_api",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "todos_api",
            "cwd": "packages/todos_api",
            "request": "launch",
            "type": "dart"
        },
        {
            "name": "todos_repository",
            "cwd": "packages/todos_repository",
            "request": "launch",
            "type": "dart"
        }
    ]
}